{
  "name": "production-control",
  "author": "Ezequias Albuquerque <ezequias.albuquerque@outlook.com>",
  "version": "0.1.0",
  "scripts": {
    "init-db": "sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "lint": "eslint .",
    "lint-fix": "eslint ./src --fix",
    "migrate": "sequelize db:migrate",
    "production": "pm2 start src/app.js --interpreter babel-node --name api-data",
    "start": "nodemon src/app.js --exec babel-node --presets env",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.3.2",
    "morgan": "^1.9.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "pm2": "^3.5.1",
    "sequelize": "^5.9.2"
  },
  "devDependencies": {
    "eslint": "5.3.0",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-plugin-import": "^2.17.2",
    "nodemon": "^1.19.1",
    "sequelize-cli": "^5.5.0"
  }
}
